{
  "extends": "eslint-config-cycle",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "object-curly-spacing": [2, "always"],
    "func-names": [0],
    "max-nested-callbacks": [2, 3],
    "quotes": [2, "single"],
    "comma-dangle": [0],
    "curly": [0],
    "no-shadow": [0]
  }
}
